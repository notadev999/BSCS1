#include<iostream>
using namespace std;
main() {
 

 string stringtext = "Grade";
 int A = 80;
 int B = 90;
 int C = 95;
 int D = 85;
 int E = A + B + C + D;
 float Y = 4;
 float Z = E / Y;

  cout << "Grades" << endl;
  cout << "Prelim:" << A << endl;
  cout << "Midterm:" << B << endl;
  cout << "Prefinal:" << C << endl;
  cout << "Final:" << D <<  endl;
  cout << "Average: " << Z << endl;

    return 0;
    }
